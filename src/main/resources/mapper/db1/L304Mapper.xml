<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.atg.apds.komipo_main.fend.mapper.db1.L304Mapper">


  <select id="getSpectrum" resultType="kr.co.atg.apds.komipo_main.entity.pobject.P_getSpectrum">
    SELECT * from public.get_spectrum(${mptkey}, #{measdt})
  </select>

  <select id="getSpectrumList" resultType="kr.co.atg.apds.komipo_main.entity.pobject.P_getSpectrumList">
    SELECT * from public.get_spectrum_list(${mptkey})
  </select>

  <select id="getSpectrumListDt" resultType="kr.co.atg.apds.komipo_main.entity.pobject.P_getSpectrumList">
    SELECT * from public.get_spectrum_list_dt(${mptkey}, #{dt})
  </select>
</mapper>